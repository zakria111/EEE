{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "button-type": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "script[type=importmap]"
        ]
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select",
          "backdrop-filter"
        ]
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "no-inline-styles": "off",
    "meta-viewport": "off",
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ]
  }
}